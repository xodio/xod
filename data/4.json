{
    "nodes": {
        "lightSensor": {
            "type": "lightSensor",
            "x": 100,
            "y": 100,
            "outputs": {
                "value": { "unit": "lx" }
            }
        },
        "mapper": {
            "type": "mapper",
            "x": 300,
            "y": 100,
