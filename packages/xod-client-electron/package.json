{
  "author": "XOD LLC <dev@xod.io>",
  "description": "XOD visual programming language IDE",
  "homepage": "https://xod.io/",
  "license": "MIT",
  "main": "dist_xod/app/main.js",
  "name": "xod-client-electron",
  "version": "0.0.1",
  "scripts": {
    "build:gui": "webpack --colors",
    "build:app": "babel src/app/ -d dist_xod/app/",
    "build": "npm-run-all build:*",
    "start": "electron ./dist_xod/app/main.js",
    "electron-rebuild": "electron-rebuild",
    "pack": "build --dir",
    "dist": "build"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "classnames": "^2.2.5",
    "electron-settings": "^3.0.14",
    "ramda": "^0.23.0",
    "rc-progress": "^2.0.6",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-event-listener": "^0.3.0",
    "react-fa": "^4.1.2",
    "react-hotkeys": "^0.9.0",
    "react-redux": "^4.4.5",
    "react-skylight": "^0.4.1",
    "redux": "^3.0.5",
    "redux-thunk": "^2.1.0",
    "serialport": "^4.0.7",
    "xod-arduino": "^0.0.1",
    "xod-arduino-builder": "^0.0.1",
    "xod-client": "^0.0.1",
    "xod-espruino-upload": "^0.0.1",
    "xod-fs": "^0.0.1",
    "xod-func-tools": "^0.0.1",
    "xod-js": "^0.0.1",
    "xod-project": "^0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "devtron": "^1.3.0",
    "electron": "^1.4.1",
    "electron-builder": "^17.1.1",
    "electron-rebuild": "^1.4.0",
    "npm-run-all": "^3.1.0",
    "webpack": "^1.13.2",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "build": {
    "appId": "io.xod.ide",
    "files": [
      "build",
      "dist_xod",
      "node_modules"
    ],
    "linux": {
      "target": [
        "deb",
        "rpm"
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}
